<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
      <toc
            file="tocconcepts.xml">
      </toc>
      <toc
            file="tocgettingstarted.xml">
      </toc>
      <toc
            file="tocreference.xml">
      </toc>
      <toc
            file="tocsamples.xml">
      </toc>
      <toc
            file="toctasks.xml">
      </toc>
      <toc
            file="tocdialogs.xml">
      </toc>
   </extension>
<extension
point="org.eclipse.ui.menus">
<menuContribution
locationURI="menu:org.eclipse.ui.main.menu">
<menu
id="help"
label="Help">
<command
commandId="org.eclipse.ui.help.displayHelp"
style="push">
</command>
</menu>
</menuContribution>
</extension>
<extension
point="org.eclipse.ui.bindings">
<key
commandId="org.eclipse.ui.help.displayHelp"
schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
sequence="F1">
</key>
</extension>
<extension
      point="org.eclipse.help.contexts">
   <contexts
         file="contexts.xml"
         plugin="com.bredexsw.examples.mailbase">
   </contexts>
</extension>
</plugin>
